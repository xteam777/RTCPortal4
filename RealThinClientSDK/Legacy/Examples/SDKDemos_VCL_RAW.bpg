MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = TcpMessages.exe UdpMessages.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

TcpMessages.exe: Raw_TCP\TcpMessages.dpr
  $(DCC)

UdpMessages.exe: Raw_UDP\UdpMessages.dpr
  $(DCC)
