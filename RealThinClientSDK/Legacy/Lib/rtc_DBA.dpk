{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DEFINE DEBUG}
{$DESCRIPTION 'RealThinClient DBA 9.5 (Legacy)'}
{$IMPLICITBUILD OFF}

requires
  rtl,
  dbrtl,
  dsnap,
  rtcSDK;

contains
  rtcDB in 'rtcDB.pas',
  rtcDBCli in 'rtcDBCli.pas',
  rtcRegisterDBA in 'rtcRegisterDBA.pas',
  rtcRegisterDBCli in 'rtcRegisterDBCli.pas';

end.
