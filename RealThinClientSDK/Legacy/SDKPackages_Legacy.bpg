#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = rtc_Raw.bpl rtc_DBA.bpl rtc_Gate.bpl
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

rtc_Raw.bpl: Lib\rtc_Raw.dpk
  $(DCC)
  
rtc_DBA.bpl: Lib\rtc_DBA.dpk
  $(DCC)

rtc_Gate.bpl: Lib\rtc_Gate.dpk
  $(DCC)