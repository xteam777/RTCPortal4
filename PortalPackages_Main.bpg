#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = rtcSDK.bpl rtcPortal.bpl 
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

rtcSDK.bpl: LibSDK\rtcSDK.dpk
  $(DCC)

rtcPortal.bpl: Lib\rtcPortal.dpk
  $(DCC)
